{
  "name": "cssnext",
  "version": "0.0.0",
  "description": "Use tomorrow's CSS syntax, today",
  "keywords": [
    "css",
    "preprocessor",
    "postprocessor",
    "rework",
    "postcss",
    "autoprefixer"
  ],
  "author": "MoOx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/putaindecode/cssnext.git"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "index.js"
  ],
  "dependencies": {
    "autoprefixer": "^2.2.0",
    "postcss": "^2.1.0",
    "postcss-calc": "^2.0.1",
    "postcss-color": "^1.0.0",
    "postcss-custom-media": "^1.0.0",
    "postcss-custom-properties": "^0.1.0",
    "postcss-import": "^1.0.0",
    "to-slug-case": "^0.1.2"
  },
  "devDependencies": {
    "jscs": "^1.5.9",
    "jshint": "^2.5.2",
    "jshint-stylish": "^0.4.0",
    "tap-colorize": "^1.2.0",
    "tape": "^2.13.4"
  },
  "scripts": {
    "standalone": "rm -rf dist && mkdir dist && browserify index.js --standalone cssnext > dist/cssnext.js",
    "jscs": "jscs *.js **/*.js",
    "jshint": "jshint . --exclude-path .gitignore --reporter node_modules/jshint-stylish/stylish.js",
    "tape": "tape test | tap-colorize",
    "test": "npm run jscs && npm run jshint && npm run standalone && npm run tape"
  }
}
